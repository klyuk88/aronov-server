{
  "kind": "collectionType",
  "collectionName": "medias",
  "info": {
    "singularName": "media",
    "pluralName": "medias",
    "displayName": "Медиа",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true
    },
    "content": {
      "type": "richtext",
      "required": true
    },
    "team": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::team.team",
      "inversedBy": "medias"
    },
    "cover": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images"
      ]
    },
    "thumbnail": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images"
      ]
    },
    "Seo": {
      "type": "component",
      "repeatable": false,
      "component": "seo.seo"
    },
    "media_categories": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::media-category.media-category",
      "inversedBy": "medias"
    },
    "shortAbout": {
      "type": "text",
      "required": true,
      "maxLength": 170
    },
    "practice": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::practice.practice",
      "inversedBy": "medias"
    },
    "slug": {
      "type": "uid",
      "required": true,
      "targetField": "title"
    }
  }
}
